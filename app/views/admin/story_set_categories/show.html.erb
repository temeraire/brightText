<div class="col-md-10 col-md-offset-1">
  <p id="notice"><%= notice %></p>

  <div class="form-group">
    <label class="control-label" for="story_set_category_name">Name</label>
    <p id="story_set_category_name" class="form-control-static"><%= @story_set_category.name %></p>
  </div>
  <div class="form-group">
    <label class="control-label" for="story_set_category_description">Description</label>
    <p id="story_set_category_description" class="form-control-static"><%= @story_set_category.description %></p>
  </div>

  <div class="row">
    <div class="col-md-2">
      <%= link_to 'Back', admin_story_set_categories_path,  {:class => "btn btn-default"}   %>
    </div>
    <div class="col-md-2">
      <%= link_to 'Edit', edit_admin_story_set_category_path(@story_set_category), {:class => "btn btn-primary"}  %>
    </div>
  </div>
</div>
