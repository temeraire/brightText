<h1>Listing domain_styles</h1>

<table>
  <tr>
    <th>Domain</th>
    <th>Style</th>
    <th>App button</th>
    <th>Groups button</th>
    <th>Sets button</th>
    <th>Stories button</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @domain_styles.each do |domain_style| %>
  <tr>
    <td><%= domain_style.domain_id %></td>
    <td><%= domain_style.style_id %></td>
    <td><%= domain_style.app_button %></td>
    <td><%= domain_style.groups_button %></td>
    <td><%= domain_style.sets_button %></td>
    <td><%= domain_style.stories_button %></td>
    <td><%= link_to 'Show', domain_style %></td>
    <td><%= link_to 'Edit', edit_admin_domain_style_path(domain_style) %></td>
    <td><%= link_to 'Destroy', domain_style_path(domain_style), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Domain style', new_domain_style_path %>
